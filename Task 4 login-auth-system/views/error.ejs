<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta
      name="description"
      content="Login Auth System is a Induction Company"
    />
    <meta name="keywords" content="Login Auth System admin panel" />
    <meta name="author" content="Decoders" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui"
    />
    <title>Login Auth System</title>
    <!-- Fevicon -->
    <link rel="shortcut icon" href="assets/images/favicon.ico" />
    <!-- Start CSS -->
    <link
      href="assets/css/bootstrap.min.css"
      rel="stylesheet"
      type="text/css"
    />
    <link href="assets/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="assets/css/style.css" rel="stylesheet" type="text/css" />
    <!-- End CSS -->
  </head>
  <body class="vertical-layout">
    <!-- Start Containerbar -->
    <div id="containerbar" class="containerbar authenticate-bg">
      <!-- Start Container -->
      <div class="container">
        <div class="auth-box error-box">
          <!-- Start row -->
          <div class="row no-gutters align-items-center justify-content-center">
            <!-- Start col -->
            <div class="col-md-8 col-lg-6">
              <div class="text-center">
                <img
                  src="assets/images/logo.jpg"
                  class="img-fluid error-logo"
                  alt="logo"
                />
                <img
                  src="assets/images/error/404.png"
                  class="img-fluid error-image"
                  alt="404"
                />
                <h4 class="error-subtitle mb-4">Oops!</h4>
                <p class="mb-4"><%= msg %></p>
                <a href="index" class="btn btn-primary font-16"
                  ><i class="ri-home-5-line mr-2"></i> Go back to Home</a
                >
              </div>
            </div>
            <!-- End col -->
          </div>
          <!-- End row -->
        </div>
      </div>
      <!-- End Container -->
    </div>
    <!-- End Containerbar -->
    <!-- Start js -->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/popper.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
    <script src="assets/js/modernizr.min.js"></script>
    <script src="assets/js/detect.js"></script>
    <script src="assets/js/jquery.slimscroll.js"></script>
    <!-- End js -->
  </body>
</html>
