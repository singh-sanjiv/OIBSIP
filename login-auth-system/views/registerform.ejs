<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="Login Auth System is a Induction Company" />
    <meta name="keywords" content="Login Auth System admin panel" />
    <meta name="author" content="Decoders" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui"
    />
    <title>Login Auth System</title>
    <!-- Fevicon -->
    <link rel="shortcut icon" href="assets/images/favicon.ico" />
    <!-- Start css -->
    <link
      href="assets/css/bootstrap.min.css"
      rel="stylesheet"
      type="text/css"
    />
    <link href="assets/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="assets/css/style.css" rel="stylesheet" type="text/css" />
    <!-- End css -->
  </head>
  <body class="vertical-layout">
    <!-- Start Containerbar -->
    <div id="containerbar" class="containerbar authenticate-bg">
      <!-- Start Container -->
      <div class="container">
        <div class="auth-box login-box">
          <!-- Start row -->
          <div class="row no-gutters align-items-center justify-content-center">
            <!-- Start col -->
            <div class="col-md-6 col-lg-5">
              <!-- Start Auth Box -->
              <div class="auth-box-right">
                <div class="card">
                    <div class="card-body">
                      <form id="userForm" action="#">
                        <!-- Name Field -->
                        <div class="form-row">
                          <div class="form-group col-md-12">
                            <label for="inputName">Name</label>
                            <input type="text" class="form-control" id="inputName" name="inputName" placeholder="Name" required>
                          </div>
                        </div>
                        
                        <!-- Email Field -->
                        <div class="form-row">
                          <div class="form-group col-md-12">
                            <label for="inputEmail">Email</label>
                            <input type="email" class="form-control" id="inputEmail" name="inputEmail" placeholder="Email" required>
                          </div>
                        </div>
                        
                        <!-- Phone Number Field -->
                        <div class="form-row">
                          <div class="form-group col-md-12">
                            <label for="inputPhoneNumber">Phone Number</label>
                            <input type="tel" class="form-control" id="inputPhoneNumber" name="inputPhoneNumber" placeholder="Phone Number" required>
                          </div>
                        </div>
                        
                        <!-- Date of Birth Field -->
                        <div class="form-row">
                          <div class="form-group col-md-12">
                            <label for="inputDateOfBirth">Date of Birth</label>
                            <input type="date" class="form-control" id="inputDateOfBirth" name="inputDateOfBirth" required>
                          </div>
                        </div>
                        
                        <!-- Password Fields -->
                        <div class="form-row">
                          <div class="form-group col-md-6">
                            <label for="inputPassword">Password</label>
                            <input type="password" class="form-control" id="inputPassword" name="inputPassword" placeholder="Password" required>
                          </div>
                          <div class="form-group col-md-6">
                            <label for="inputPasswordConfirm">Confirm Password</label>
                            <input type="password" class="form-control" id="inputPasswordConfirm" name="inputPasswordConfirm" placeholder="Confirm Password" required>
                          </div>
                        </div>
                        
                        <!-- Submit Button -->
                        <button type="submit" class="btn btn-primary">Create</button>
                      </form>
                      
                    </div>
               
                     
                    <p class="mb-0 mt-3">
                      Already have a account?
                      <a href="/login">Login Here</a>
                    </p>
                  </div>
                </div>
              </div>
              <!-- End Auth Box -->
            </div>
            <!-- End col -->
          </div>
          <!-- End row -->
        </div>
      </div>
      <!-- End Container -->
    </div>
    <!-- End Containerbar -->
    <!-- Start js -->
    
    <script src="/js/axios.min.js"></script>
    <script src="/js/register.js"></script>
    <!-- <script src="/js/index.js"></script> -->
    <!-- <script src="/js/bundle.js"></script> -->
    <!-- <script src="/js/bundle.js.map"></script> -->

    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/popper.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
    <script src="assets/js/modernizr.min.js"></script>
    <script src="assets/js/detect.js"></script>
    <script src="assets/js/jquery.slimscroll.js"></script>
    <!-- End js -->
  </body>
</html>
